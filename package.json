{
  "name": "create-next-example-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "./node_modules/.bin/next build && ./node_modules/.bin/sw-precache --navigate-fallback='index.html' --root='.next/' --static-file-globs='.next/**/!(*map*)'",
    "start": "npm run build && NODE_ENV=production node server.js",
    "build-css": "node-sass ./assets/styles/ -o ./assets/styles/",
    "watch-css": "npm run build-css && node-sass ./assets/styles/ -o ../assets/styles/ --watch --recursive",
    "generate-icons": "icon-font-generator ./assets/icons/*.svg -o ./assets/fonts"
  },
  "devDependencies": {
    "https": "^1.0.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "pushstate-server": "^1.14.0",
    "react-scripts": "0.7.0",
    "react-snapshot": "^1.0.4",
    "socket.io": "^1.7.3",
    "sw-precache": "^4.2.1"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "express": "^4.15.3",
    "google-vision-api-client": "0.0.3",
    "history": "^4.6.1",
    "icon-font-generator": "^1.0.7",
    "jquery": "^3.2.1",
    "multer": "^1.3.0",
    "next": "^2.4.0",
    "next-redux-wrapper": "^1.1.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-cloud-vision-api": "^0.3.0",
    "react-datepicker": "^0.46.0",
    "react-dates": "^11.0.1",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.13.1",
    "react-fullpage": "^0.1.16",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-transition": "0.0.6",
    "react-slick": "^0.14.11",
    "react-touch": "^0.4.2",
    "redux": "^3.6.0",
    "redux-file-upload": "0.0.18",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "unsplash-api": "^1.2.0",
    "unsplash-js": "^4.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "homepage": "/",
  "watch": {
    "generate-icons": "./src/assets/icons/*.svg"
  }
}
